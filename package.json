{
    "name": "peymate-on-icp",
    "private": true,
    "version": "0.1.0",
    "scripts": {
        "build": "cd src/frontend && VITE_CANISTER_ID=$(dfx canister id express) vite build",
        "pretest": "tsx test/pretest.ts",
        "nest:build": "cd src/backend && nest build",
        "nest:start": "cd src/backend && nest start",
        "nest:start:dev": "cd src/backend && nest start --watch",
        "nest:start:debug": "cd src/backend && nest start --debug --watch",
        "nest:start:prod": "node src/backend/dist/main",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@dfinity/auth-client": "^2.2.0",
        "@dfinity/identity": "^2.2.0",
        "@dfinity/principal": "^2.2.0",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.0.0",
        "@nestjs/platform-express": "^10.0.0",
        "@ton/core": "^0.59.0",
        "@ton/crypto": "^3.3.0",
        "@ton/ton": "^15.1.0",
        "azle": "0.24.1",
        "buffer": "^6.0.3",
        "crypto": "^1.0.1",
        "express": "^4.18.2",
        "get-random-values": "^3.0.0",
        "lit": "^3.1.1",
        "reflect-metadata": "^0.2.0",
        "rxjs": "^7.8.1"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/supertest": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "jest": "^29.7.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.4",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.15.7",
        "typescript": "^5.2.2",
        "vite": "^5.0.12"
    }
}
